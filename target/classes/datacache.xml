<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- 数据缓存的配置 -->
<CacheConfigs>
    <client>
        <factory>com.sohu.mp.datacache.impl.RedisCacheClientFactory</factory>
        <properties>
            <property key="servers" value="10.16.1.235:7379"/>
             
            <property key="timeout" value="0"/>
            
            <property key="maxActive" value="500"/>
            <property key="minIdle" value="0"/>
            <property key="maxIdle" value="500"/>
            <property key="maxWait" value="2000"/>
        </properties>
    </client>

    <cache name="MP_ACCOUNTS" cacheTimeSeconds="0" keyGeneratorClass="com.sohu.mp.cache.impl.DefaultKeyGenerator"/>
    <cache name="ACCOUNTS_SYNC" cacheTimeSeconds="0" keyGeneratorClass="com.sohu.mp.cache.impl.DefaultKeyGenerator"/>
    
</CacheConfigs>
